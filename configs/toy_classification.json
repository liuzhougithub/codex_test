{
  "seed": 13,
  "device": "cpu",
  "max_epochs": 5,
  "log_every_n_steps": 10,
  "output_dir": "outputs/toy",
  "dataset": {
    "target": "classification.data.ToyClassificationDataModule",
    "params": {
      "num_samples": 1500,
      "num_features": 32,
      "num_classes": 4,
      "batch_size": 128,
      "val_split": 0.15,
      "test_split": 0.15,
      "seed": 13
    }
  },
  "model": {
    "target": "classification.models.MLPClassifier",
    "params": {
      "in_features": 32,
      "hidden_dims": [256, 128, 64],
      "num_classes": 4,
      "dropout": 0.2
    }
  },
  "optimizer": {
    "target": "torch.optim.Adam",
    "params": {
      "lr": 0.001
    }
  }
}
